type Result = 
 variant {
   err: text;
   ok: text;
 };
type ConfigurationTypes = 
 variant {
   boolean;
   number;
   string;
 };
service : {
  createApplication: (text, text) -> (text);
  createConfiguration: (text, text, text, ConfigurationTypes) -> (text);
  createEnvironment: (text, text, text) -> (text);
  getConfigValue: (text, text, text) -> (opt text);
  setConfigValue: (text, text, text, text) -> (Result);
}
